{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Emojis",
            "program": "${workspaceFolder}/extract-emojis/out/emoji.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Extract",
            "program": "${workspaceFolder}/extract-emojis/out/extract.js"
        },
        {
            "name": "Launch in Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8100",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/www",
        }
    ]
}
